{
    "workbench.colortheme": "dracula",
    "window.zoomlevel": 0,
    "autoalign.enabled": {
        "csv": true,
        "bsv": true,
        "json": false
    },
    "autoalign.associations": {
        "csv": ",",
        "bsv": "|",
        "json": ","
    },
"explorer.confirmDelete": false,
"editor.suggestSelection": "first",
"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
"files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
},
"explorer.confirmDragAndDrop": false,
"window.zoomLevel": 0,
"workbench.colorTheme": "Dracula Soft",
"vim.useSystemClipboard": true,
"vim.insertModeKeyBindings": [
    {
        "before": ["j", "k"],
        "after": ["<Esc>"]
    }
],
"vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["leader", "b", "n"],
        "commands": [
            "workbench.action.nextEditor",
        ]
    },
    {
        "before": ["leader", "b", "p"],
        "commands": [
            "workbench.action.previousEditor",
        ]
    },
    {
        "before": [":"],
        "commands": [
            "workbench.action.showCommands",
        ]
    },
    {
        "before": ["Z","Z"],
        "commands": [
            ":wq",
        ]
    },
    {
        "before": ["leader","w"],
        "commands": [
            "workbench.action.files.save",
        ]
    }
],
"vim.showmodename": true,
"vim.autoindent": true,
"vim.ignorecase": true,
"vim.incsearch": true,
"vim.smartcase": true,
"vim.showcmd": true,
"vim.debug.silent": true,
"vim.leader": "<space>",
"editor.lineNumbers": "relative",
"editor.largeFileOptimizations": false,
"diffEditor.ignoreTrimWhitespace": false,
"editor.multiCursorModifier": "ctrlCmd",
"workbench.editor.enablePreview": false,
"python.jediEnabled": false,
"workbench.editor.enablePreviewFromQuickOpen": false,
"workbench.editor.highlightModifiedTabs": true,
"workbench.startupEditor": "none",
"yaml.schemas": {
    "file:///Users/sduran/.vscode/extensions/atlassian.atlascode-2.6.6/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
},
"atlascode.jira.jqlList": [
    {
        "id": "4eb476c3-d6e1-4b3c-9b64-54721b5922fb",
        "enabled": true,
        "name": "My rockethomes Issues",
        "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
        "siteId": "d6d799df-496c-4757-8a9c-4ac8a1504935",
        "monitor": true
    }
],
"git.autofetch": true,
"java.semanticHighlighting.enabled": false,
"python.languageServer": "Microsoft",
"files.trimTrailingWhitespace": true,
"workbench.iconTheme": "material-icon-theme"
}
